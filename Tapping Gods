spawn(
    function()
        while wait() do
            if AutoGems then
                for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
                    if v.Name == "Diamond" then
                        v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                    end
                end
            end
        end
    end
)
